{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noEmitOnError": false,
        "noLib": false,
        "sourceMap": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": [
            "dom",
            "es2016"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}